<div>
<div align="center">
<h1 >Daily Records</h1>
<h4> Total Daily Records: {{ daily_records_count }}</h4>
</div> 
<div align="right">
    <h3 ><a href="{{ 'users' | route }}">Users List</a></h3>
</div>
</div>
<div align="center">
<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Male Count</th>
      <th>Female Count</th>
      <th>Male Avg Age</th>
      <th>Female Avg Age</th>
      <th>Created At</th>
      <th>Last Update At</th>
    </tr>
  </thead>
  <tbody>
    {% for daily_record in daily_records %}
      <tr>
        <td>{{ daily_record.date | date: "%Y-%m-%d" }}</td>
        <td>{{ daily_record.male_count }}</td>
        <td>{{ daily_record.female_count }}</td>
        <td>{{ daily_record.male_avg_age }}</td>
        <td>{{ daily_record.female_avg_age }}</td>
        <td>{{ daily_record.created_at | date: "%Y-%m-%d %H:%M" }}</td>
        <td>{{ daily_record.updated_at | date: "%Y-%m-%d %H:%M" }}</td>
      </tr>
    {% endfor %}
  </tbody>
</table>
</div>